
<h2><%= link_to plural_resource_name(Spree::Provider), admin_providers_path %> /
  <%= @provider.nome_fantasia %>
</h2>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @provider } %>

<%= form_for [:admin, @provider], method: :put, html: { multipart: true } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
